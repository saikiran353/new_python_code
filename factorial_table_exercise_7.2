from decimal import *
import math
def mysquare_root(a):
    x = 1
    while True:
        y = (x + a/x) / 2
        if x == y:
            break
        x = y
    return x

def square_root(a):
    x = math.sqrt(a)
    return x
def difference(a):
    d = square_root(a) - mysquare_root(a)
    return d

