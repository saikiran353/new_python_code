    d = Decimal(mysquare_root(a))
    #print(d)
    d1 = round(d, 15)
    #print(d1)
    if d1 - Decimal(mysquare_root(a)) == 0:
        d1 = round(d, 0)
        #print(d1)
        return float(d1)
    #print(d1)
    return (d1)
